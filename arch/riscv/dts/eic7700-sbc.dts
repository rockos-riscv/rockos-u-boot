#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/soc/eswin-pmp.h>
#include "eic770x.dtsi"
// #include "dt-bindings/media/eswin_mipi_dsi.h"
// #include "linux/media-bus-format.h"
/ {
	#address-cells = <2>;
	#size-cells = <2>;
	model = "ESWIN EIC7700 SBC";
	compatible = "eswin,eic7700", "eswin,eic7700-sbc";

	aliases {
		serial0 = &uart0;
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		i2c4 = &i2c4;
		i2c5 = &i2c5;
		i2c6 = &i2c6;
		i2c7 = &i2c7;
		i2c8 = &i2c8;
		i2c9 = &i2c9;
		i2c11 = &i2c11;
		i2c12 = &i2c12;
		gpio0 = &gpio0;
		spi0 = &bootspi;
	};

	config {
		power-gpios = <&portd 10 GPIO_ACTIVE_HIGH>;
	};

	chosen {
		stdout-path = "serial0";
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x4 0x00000000>;
	};
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		display_reserved: region@E0000000 {
			reg = <0x0 0xE0000000 0x0 0x1000000>;
			no-map;
		};

		lpcpu_reserved: lpcpures@DFFF0000 {
			reg = <0x00 0xDFFF0000 0x00 0x00010000>;
			no-map;
		};
	};
	soc {
		#address-cells = <2>;
		#size-cells = <2>;
		compatible = "SiFive,FU800-soc", "fu800-soc", "sifive-soc", "simple-bus";
		ranges;
	};
	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio24_default>;

		gpio-84 {
			gpios = <&porta 24 GPIO_ACTIVE_HIGH>;
			label = "gpio-84";
			linux,default-trigger = "default-on";
		};
	};
};
&cpu0 {
	status = "okay";
};
&cpu1 {
	status = "okay";
};
&cpu2 {
	status = "okay";
};
&cpu3 {
	status = "okay";
};
&plic0{
	status = "okay";
};
&d0_mbox {
	status = "okay";
};
&d0_mbox_srvc {
	status = "okay";
};
&d0_mbox1 {
	status = "okay";
};
&d0_mbox_srvc1 {
	status = "okay";
};
&d0_pmp {
	status = "okay";
};
&uart0 {
	status = "okay";
};

&bootspi {
	status = "okay";
	cs-gpios = <&portd 0 GPIO_ACTIVE_LOW>;
	wp-gpios = <&portd 4 GPIO_ACTIVE_LOW>;
	spi-flash@0 {
		reg = <0>;
		compatible = "winbond,w25q64dw",
			"jedec,spi-nor";
		spi-max-frequency = <4800000>;
	};
};
&sd {
	status = "okay";
	delay_code = <0x55>;
	drive-impedance-ohm = <33>;
	enable-cmd-pullup;
	enable-data-pullup;
};

&eswin_sata {
	status = "okay";
};

&eth0 {
	status = "okay";
	phy-mode = "rgmii-id";
	max-speed = <1000>;
	phy-handle = <&phy0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpio106_default>;
	rst-gpios = <&portd 10 GPIO_ACTIVE_LOW>;
	dly-param-1000m = <0x23232323 0x800c8023 0x0c0c0c0c>;
	dly-param-100m = <0x50505050 0x803f8050 0x3f3f3f3f>;
	dly-param-10m = <0 0 0>;
	mdio0 {
		#address-cells = <1>;
		#size-cells = <0>;
		phy0: ethernet-phy@1 {
			reg = <0>;
		};
	};
};

&pcie {
	status = "okay";
};

&display_logo {
	status = "disabled";
};
&dwc3_typec0 {
		status = "okay";
		dr_mode = "peripheral";
		maximum-speed = "super-speed";
};
&usb0 {
	status = "okay";
};
&usb1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpio23_default>;
	hub-rst-gpios = <&porta 23 GPIO_ACTIVE_HIGH>;
};
&hdmi {
	status = "okay";
};
&display_subsystem {
	status = "okay";
};
&route_hdmi {
	status = "okay";
};
&osd {
	status = "okay";
};
/*
GPIO USED ON CarrierBoard:
	gpio0   : FP Audio Jack Sense(I), active low
	gpio5   : TYPE C cc logic interrupt(I), active low
	gpio11  : BT WAKE HOST(I), active low
	gpio12  : PCIE present(I), active low
	gpio14  : DSI FPC CON CTRL(J10&J11)
	gpio15  : Wlan wake host(I), active low
	gpio28  : RP audio jack sense(I), active low
	gpio29  : EMMC active led ctrl(O)

	gpio43  : USB3.2 Gen1 hub Resetn(O), active low
	gpio71  : CSI fpc con ctrl(O)
	gpio74  : CSI fpc con ctrl(O)
	gpio77  : CSI fpc con ctrl(O)
	gpio76  : HOST WAKE BT(O), active low
	gpio79  : WLAN POWER ON(O), active high
	gpio80  : CSI fpc con ctrl(O)
	gpio82  : DSI FPC CON CTRL(J10)
	gpio85  : DSI FPC CON CTRL(J11)
	gpio84  : GPIO LED CTRL(O), active high

 GPIO USED ON SOM:
	gpio18  : HOST WAKE WLAN(O), active low
	gpio19  : HOST WAKE BT(O), active low
	gpio20  : WLAN WAKE HOST(I), active low
	gpio21  : BT WAKE HOST(I), active low
	gpio106 : gphy0 resern(O), active low
	gpio111 : gphy1 resern(O), active low

*/

&gpio0 {
	status = "okay";
};

&pinctrl {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpio94_default>;
};